@page 
@model app.Pages.Logradouro.EditarModel
@{
    ViewData["Title"] = "Editar Logradouro";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Logradouro.endereco"></label>
        <input asp-for="Logradouro.endereco" class="form-control" />
        <input type="hidden" asp-for="Logradouro.idCliente" class="form-control" />
        <input type="hidden" asp-for="Logradouro.id" class="form-control" />
    </div>    
    <button type="submit" class="btn btn-primary">Salvar</button>
</form>

